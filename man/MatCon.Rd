p<-MatCon$new(A,Source="Labatut, V., & Cherifi, H. (2011). Evaluation of performance measures for classifiers comparison. arXiv preprint arXiv:1112.4133.")
p$Sucess()

}
\if{html}{\out{</div>}}

}

}
